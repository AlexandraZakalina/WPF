<Window x:Class="UchP11.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UchP11"
        mc:Ignorable="d"
        Title="MainWindow" Height="auto" Width="1000">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="2"></Grid>
        <Button Name="Button1" Height="50" Grid.Row="1" Width="300" Background="#fff" Click="Button1Click" RenderTransformOrigin="1,1">Войти</Button>
        <Frame ContentRendered="MainFrame_ContentRendered"  Grid.Row="1" Name="MainFrame"></Frame>
        
        
    </Grid>
</Window>
